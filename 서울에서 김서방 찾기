#include <string>
#include <vector>

using namespace std;

string solution(vector<string> seoul) {//string 형의 solution이라는 함수 선언
    string answer = ""; //answer라는 변수 선언 후 초기화
    
    for (int i=0;i<seoul.size();i++){
        if(seoul[i]=="Kim")
            
        answer = "김서방은 " +to_string(i)+ "에 있다" ;
    }

    return answer;
}
